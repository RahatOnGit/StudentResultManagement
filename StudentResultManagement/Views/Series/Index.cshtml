@model IEnumerable<Series>

<br />
<br />

<div class="row">
    <div class="col-6">
        <h2 class="text-info">Series Name</h2>
    </div>
    <div class="col-6 text-right">
        <a asp-action="Create" class="btn btn-info"><i class="fa-solid fa-plus"></i>&nbsp; New Series</a>
    </div>
</div>

<br />

<div>
    <table class="table table-striped border">
        <tr class="table-info">
            <th>
                @Html.DisplayNameFor(c => c.SeriesName)
            </th>

            <th></th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
                <tr>
                    <td>@item.SeriesName</td>
                     <td><partial name="buttonforEditDelUpdate" model="@item.Id" /></td> 
                </tr>
        }

    </table>
</div>